{
  "name": "pluma/cms",
  "description": "Pluma - A Web CMS",
  "version": "2.0.1",
  "homepage": "https://github.com/lioneil/pluma",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "John Lioneil Dionisio",
      "email": "john.dionisio1@gmail.com"
    },
    {
      "name": "Princess Ellen Alto",
      "email": "princessalto@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.4",
    "asm89/stack-cors": "^1.2",
    "chumper/zipper": "^1.0",
    "doctrine/inflector": "~1.0",
    "dompdf/dompdf": "^0.8.2",
    "filp/whoops": "^2.1",
    "illuminate/auth": "^5.5",
    "illuminate/bus": "^5.5",
    "illuminate/cache": "^5.5",
    "illuminate/config": "^5.5",
    "illuminate/container": "^5.5",
    "illuminate/contracts": "^5.5",
    "illuminate/cookie": "^5.5",
    "illuminate/database": "^5.5",
    "illuminate/encryption": "^5.5",
    "illuminate/events": "^5.5",
    "illuminate/hashing": "^5.5",
    "illuminate/log": "^5.5",
    "illuminate/mail": "^5.5",
    "illuminate/notifications": "^5.5",
    "illuminate/pagination": "^5.5",
    "illuminate/queue": "^5.5",
    "illuminate/routing": "^5.5",
    "illuminate/support": "^5.5",
    "illuminate/validation": "^5.5",
    "illuminate/view": "^5.5",
    "mtdowling/cron-expression": "^1.2",
    "nesbot/carbon": "^1.22",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "~3.0",
    "robmorgan/phinx": "^0.8.0",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/polyfill-mbstring": "^1.7",
    "symfony/var-dumper": "^3.0",
    "vlucas/phpdotenv": "^2.4",
    "wikimedia/composer-merge-plugin": "^1.4"
  },
  "autoload": {
    "classmap": [
      "core",
      "modules",
      "blacksmith"
    ],
    "psr-4": {
      "Pluma\\": "core/"
    }
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "composer.local.json",
        "core/submodules/*/composer.json",
        "modules/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": true,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php pluma key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
